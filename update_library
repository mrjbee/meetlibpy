echo "== Move to library folder"
echo "(pwd)"
file_name=commands.list
/opt/bin/git fetch
/opt/bin/git reset --hard origin/library_ddwrt
rm -rf *.pyc
while read command
do
    echo "== Merging last version of '$command'"
    /opt/bin/git merge -m "Merge command $command" origin/commands/$command
done < $file_name

echo ""
echo "== Commands package:"
ls -l
